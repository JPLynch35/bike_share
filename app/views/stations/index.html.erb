<% @stations.each do |station| %>
<ul>
  <li> Name: <%= station.name %>
  <li> City: <%= station.city %>
  <li> Dock Count: <%= station.dock_count %>
  <li> Installation Date:<%= station.installation_date %>
  <% if current_admin? %>
    <%= button_to "Edit", edit_admin_station_path(station) %></p>
    <%= button_to "Delete", admin_station_path(station), method: "delete", data: {confirm: "Are you sure you want to delete this station?"} %></p>
  <% end %>
</ul>
<% end %>
