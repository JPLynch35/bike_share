<h1>Bike Shop</h1>

<ul>
  <% @accessories.each do |accessory| %>
  <div id="accessory_<%= accessory.id %>">
    <li>Image tag placeholder</li>
    <li><%= accessory.title %></li>
    <li><%= accessory.description %></li>
    <li><%= accessory.price %></li>
    <li><%= link_to 'Add to Cart (placeholder link path)', bike_shop_path %></li>
  </div>
  <% end %>
</ul>
